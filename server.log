<style>
  .exit-container {
    max-width: 800px;
    margin: 0 auto;
    padding: 20px;
  }
  .entries-table {
    margin-top: 20px;
  }
</style>
<h2 class="text-center mb-4">Vehicle Exit</h2>
<div class="exit-container">
  <% if (error) { %>
    <div class="alert alert-danger text-center"><%= error %></div>
  <% } %>
  <!-- Entries Table -->
  <table class="table table-bordered entries-table">
    <thead>
      <tr>
        <th>Number Plate</th>
        <th>Entry Time</th>
        <th>Owner Name</th>
        <th>Phone</th>
        <th>Category</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <% if (entries && entries.length > 0) { %>
        <% entries.forEach(entry => { %>
